{#
/**
 * @file
 * Theme override for a views exposed form.
 *
 * Available variables:
 * - form: A render element representing the form.
 *
 * @see template_preprocess_views_exposed_form()
 *
 * @ingroup themeable
 */
#}
{% if q is not empty %}
	{# This ensures that, if clean URLs are off, the 'q' is added first,as a hidden form element, so that it shows up first in the POST URL. #}

	{{ q }}

{% endif %}

{% set classes = [
	    'filter',
	] %}

<div {{attributes.addClass(classes)}} id="services">
	
	{% set keys = form.keys %}
	{% set keys = keys|merge({
  '#attributes': keys['#attributes']|merge({
    'class': (keys['#attributes'].class|default([]))|merge(['mb-4 h-[52px] w-full rounded-sm border border-[#D4D4D8] bg-[#fff] bg-[url(../images/Search.svg)] bg-[16px_center] bg-no-repeat py-[18px] pe-2 ps-11 text-[#101828] placeholder:text-[#101828] md:mb-10 md:max-w-[333px]'])
  })
}) %}

	{{ form.service_type }}	
		{{ keys }}
	{# {% set service_type = form.service_type %} #}

		{# {{ form.service_type }} #}
	<div class="submit-reset hidden ">
		{{ form.actions }}
	</div>
	{{ form|without('actions','keys','service_type') }}
</div>
